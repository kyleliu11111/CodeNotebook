#include <bits/stdc++.h>

using namespace std; 

const int MAXN = 1e5 + 1;

int N, Q;
int T[MAXN*4], V[MAXN];

void build(int x, int l, int r) {
    if(l == r) {
        T[x] = V[l];
        return;
    }
    int md = (l+r)/2;
    if(l <= md) build(x*2, l, md);
    if(md < r) build(x*2+1, md+1, r);
    T[x] = min(T[x*2], T[x*2+1]);
}

int get_min(int x, int l, int r, lq, int rq) {
    if(l > rq || r < lq) return INT_MAX;
    if(lq <= l && rq >= r) return T[x];
    int md = (l+r)/2;
    return min(get_min(x*2, l, md, lq, rq), get_min(x*2+1, md+1,  r, lq, rq));
}

int main(void)
{
    cin >> N >> Q;
    for(int i=1; i<=N; i++) cin >> V[i];
    build(1, 1, N);
    for(int i=0; i<Q; i++) {
        int lq, rq;
        cin >> lq >> rq;
        cout << get_min(1, 1, N, lq, rq) << '\n';
    }
}
